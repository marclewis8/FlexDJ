{
  "name": "backend",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "devDependencies": {
    "@types/node": "^8.0.29",
    "ts-node": "3.3.0",
    "typescript": "3.3.3333"
  },
  "dependencies": {
    "pg": "^7.3.0",
    "reflect-metadata": "^0.1.10",
    "typeorm": "0.2.28",
    "uuid": "^8.3.1"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "cd test && npm run test",
    "typeorm:migrate": "npm run typeorm migration:generate -- -n",
